  "scripts": {
    "assess": "babel-node node_modules/.bin/code-assess src",
    "buildProd": "webpack --mode production --progress --config webpack.prod.config.js src/client",
    "clean": "./utilities/clean.sh",
    "fullClean": "npm run clean && npm run rebuildDb && npm run setup",
    "incMinor": "python3 utilities/version.py minor",
    "incPatch": "python3 utilities/version.py patch",
    "installGitHooks": "printf '%s#!/bin/bash %s\n %s\necho \"running linter\" %s\nnpm run lint' > .git/hooks/pre-push && chmod +x .git/hooks/pre-push",
    "jest": "jest src/client",
    "lint": "./node_modules/.bin/eslint src/ && flake8 server/",
    "loadFixtures": "python3 utilities/loadFixtures.py",
    "rebuildDb": "python3 server/dbUtils.py rebuild",
    "runProd": "npm run buildProd && npm run startServer'",
    "setup": "npm install && npm run installGitHooks && npm run rebuildDb",
    "start": "npm run watchClient & npm run startServer",
    "startServer": "python3 server/server.py",
    "test": "npm run jest",
    "tests": "npm run lint && npm run test",
    "watchClient": "webpack --mode development --watch --progress --config webpack.dev.config.js src/client",
    "watchProd": "webpack --mode production --watch --progress --config webpack.prod.config.js src/client"
  },
