  "scripts": {
    "build:prod": "webpack --mode production --progress --config webpack.prod.config.js src/client",
    "clean": "./utilities/clean.sh",
    "clean:all": "npm run clean && npm run rebuild:db && npm run setup",
    "inc:minor": "python3 utilities/version.py minor",
    "inc:patch": "python3 utilities/version.py patch",
    "install:git-hooks": "printf '%s#!/bin/bash %s\n %s\necho \"running linter\" %s\nnpm run lint' > .git/hooks/pre-push && chmod +x .git/hooks/pre-push",
    "jest": "jest src/client",
    "lint": "./node_modules/.bin/eslint src/ && flake8 server/",
    "load:fixtures": "python3 utilities/loadFixtures.py",
    "prettier": "prettier --list-different \"**/*.{js,md}\"",
    "rebuild:db": "python3 server/dbUtils.py rebuild",
    "start:prod": "npm run build:prod && npm run start:server",
    "setup": "npm install && npm run install:gitHooks && npm run rebuild:db",
    "start": "npm run watch:client & npm run start:server",
    "start:server": "python3 server/server.py",
    "test": "npm run jest",
    "tests": "npm run lint && npm run test",
    "watch:client": "webpack --mode development --watch --progress --config webpack.dev.config.js src/client",
    "watch:prod": "webpack --mode production --watch --progress --config webpack.prod.config.js src/client"
