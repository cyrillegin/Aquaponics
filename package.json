  "name": "Dragonfly",
  "private": true,
  "repository": "",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:client\" \"npm run start:server:dev\"",
    "start:server:dev": "nodemon ./dist/index.js",
    "start:satellite": "python3 satellite",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "watch:client:prod": "webpack --watch --progress --config webpack.prod.config.js src/client",
    "watch:server": "babel --watch src/server --out-dir dist/ --copy-files",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src",
    "lint:styles": "stylelint src/**/*.js",
    "prettier": "prettier --list-different \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.js\"",
    "test": "npm run prettier && npm run lint && npm run test:unit",
    "test:unit": "jest src/ --coverage",
    "test:update": "jest src/ -u",
    "watch:tests": "jest src/ --watch  --coverage --runInBand",
    "generate:fixtures": "babel-node utility/generateFixtures.js",
    "db:shell": "docker exec -it dragonfly_postgres_1 psql db -U user",
    "db:up": "sequelize db:migrate --env 'development'",
    "db:down": "sequelize db:migrate:undo:all --env 'development'",
    "db:status": "sequelize db:migrate:status --env 'development'",
    "db:new:migration": "sequelize migration:generate --name migration-skeleton",
    "build:server:prod": "babel src/server --out-dir dist/ --copy-files",
    "build:client:prod": "webpack --progress --config webpack.prod.config.js src/client",
    "build:satellite:prod": "",
    "start:server:prod": "node dist/index.js",
    "start:satellite:prod": "python3 satellite/"
  },
  "version": "0.0.1"
}
