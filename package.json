  "scripts": {
    "build:prod": "npm run build:prod-server && npm run build:prod-client",
    "build:prod-client": "webpack --mode production --progress --config webpack.prod.config.js src/client",
    "build:prod-server": "babel src/ --out-dir dist --copy-files --ignore src/client/ --source-maps inline",
    "fixtures": "babel-node utilities/fixtures.js",
    "inc:minor": "python utilities/version.py minor",
    "inc:patch": "python utilities/version.py patch",
    "lint": "eslint src",
    "prettier": "prettier --list-different \"**/*.{js,md}\"",
    "start": "npm run watch:server & npm run watch:client",
    "start:prod": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/server",
    "test": "npm run lint",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "watch:server": "cp -a src/server/public/. dist/server/public & babel-watch --watch src/server/ src/server/index.js"
