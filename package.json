{
  "author": "Cyrille Gindreau",
  "dependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "10.0.21",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "2.0.0",
    "concurrently": "5.0.0",
    "dotenv": "6.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "express": "4.17.1",
    "jest": "24.1.0",
    "jest-emotion": "10.0.17",
    "jest-fetch-mock": "2.1.2",
    "jest-junit": "6.4.0",
    "nodemon": "2.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.8.4",
    "sequelize": "^5.21.10",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.14.2",
    "uuid": "3.3.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.11.0"
  },
  "description": "",
  "devDependencies": {},
  "license": "",
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "!**/__snapshots__/**",
      "!**/dist/**",
      "!**/Demo.js",
      "!**/node_modules/**",
      "packages/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/packages/atoms/dist",
      "<rootDir>/packages/classes/dist",
      "<rootDir>/packages/constants/dist",
      "<rootDir>/packages/functions/dist",
      "<rootDir>/packages/hocs/dist",
      "<rootDir>/packages/layouts/dist",
      "<rootDir>/packages/molecules/dist",
      "<rootDir>/packages/organisms/dist",
      "<rootDir>/packages/styles/dist",
      "<rootDir>/packages/svgs/dist"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>utility/mockHelpers.js",
      "<rootDir>utility/setupTests.js"
    ],
    "verbose": false
  },
  "name": "Dragonfly",
  "private": true,
  "repository": "",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:client\" \"npm run start:server:dev\"",
    "start:server:prod": "node ./src/index.js",
    "start:server:dev": "nodemon ./dist/index.js",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "watch:server": "babel --watch src/server --out-dir dist/ --copy-files"
  },
  "version": "0.0.1"
}
