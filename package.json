    "pythonlint": "find ./server -name '*.py' | xargs pep8 --max-line-length=200 --exclude='server/commands/dht11.py'", 
    "scsslint": "./node_modules/.bin/sass-lint src '**/*.scss' -v -q", 
    "start": "concurrently --kill-others 'npm run buildDev ' 'python3 server/run.py run'", 
    "startProd": "concurrently --kill-others 'npm run buildProd' 'python3 server/run.py run'", 
    "watch": "webpack --watch --progress --config ./webpack.dev.config.js", 
    "webpack": "webpack --progress"
