{
  "author": "Cyrille Gindreau",
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "4.0.0",
    "concurrently": "5.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "pg": "8.2.1",
    "pg-hstore": "2.3.3",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "sequelize": "5.21.10",
    "uuid": "8.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "description": "",
  "devDependencies": {
    "@babel/polyfill": "7.8.7",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "jest": "26.0.1",
    "jest-emotion": "10.0.32",
    "jest-junit": "10.0.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "stylelint": "13.5.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.17.2",
    "supertest": "4.0.2",
    "webpack-dev-server": "3.11.0"
  },
  "license": "",
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "!**/__snapshots__/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleNameMapper": {},
    "modulePathIgnorePatterns": [],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>utility/setupTests.js"
    ],
    "verbose": false
  },
  "name": "Dragonfly",
  "private": true,
  "repository": "",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:client\" \"npm run start:server:dev\"",
    "start:server:prod": "node ./src/index.js",
    "start:server:dev": "nodemon ./dist/index.js",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "watch:server": "babel --watch src/server --out-dir dist/ --copy-files",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src",
    "lint:styles": "stylelint src/**/*.js",
    "prettier": "prettier --list-different \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.js\"",
    "test": "npm run prettier && npm run lint && npm run test:unit",
    "test:unit": "jest src/ --coverage",
    "test:update": "jest src/ -u",
    "watch:tests": "jest src/ --watch  --coverage"
  },
  "version": "0.0.1"
}
