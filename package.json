{
  "author": "Cyrille Gindreau",
  "dependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@emotion/core": "10.0.28",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "10.0.21",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "2.0.0",
    "concurrently": "5.0.0",
    "dotenv": "6.2.0",
    "express": "4.17.1",
    "pg": "8.2.1",
    "pg-hstore": "2.3.3",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-router-dom": "4.3.1",
    "sequelize": "5.21.10",
    "uuid": "3.3.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1"
  },
  "description": "",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "jest": "24.1.0",
    "jest-emotion": "10.0.17",
    "jest-fetch-mock": "2.1.2",
    "jest-junit": "6.4.0",
    "nodemon": "2.0.4",
    "react-test-renderer": "16.8.4",
    "prettier": "1.19.1",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.14.2",
    "webpack-dev-server": "3.11.0"
  },
  "license": "",
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "!**/__snapshots__/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {

    },
    "modulePathIgnorePatterns": [

    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>utility/setupTests.js"
    ],
    "verbose": false
  },
  "name": "Dragonfly",
  "private": true,
  "repository": "",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:client\" \"npm run start:server:dev\"",
    "start:server:prod": "node ./src/index.js",
    "start:server:dev": "nodemon ./dist/index.js",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "watch:server": "babel --watch src/server --out-dir dist/ --copy-files",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src",
    "lint:styles": "stylelint src/**/*.js",
    "prettier": "prettier --list-different \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.js\"",
    "test": "npm run prettier && npm run lint && npm run test:unit",
    "test:unit": "jest src/ --coverage",
    "test:update": "jest src/ -u",
    "watch:tests": "jest src/ --watch  --coverage"
  },
  "version": "0.0.1"
}
